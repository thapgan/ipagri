{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "warn",
        "operator-linebreak": "off",
        "no-console": "off",
        "prefer-destructuring": ["off", {
            "array": true,
            "object": true
          }, {
            "enforceForRenamedProperties": false
        }],
        "no-underscore-dangle": "off",
        "comma-dangle": "off"
    }
}
